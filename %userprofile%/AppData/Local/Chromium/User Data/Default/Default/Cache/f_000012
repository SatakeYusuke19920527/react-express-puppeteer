		    ///<reference path="Scripts/jquery-1.4.1.min.js" />
		    var _fww = true;
		    var nFiles;
			var strResult;
			var bCondError;
			var bReadOnly = false;
			var _busy = false;
            var _ctrlKey = false;
			var termType = 1;    //  1が自分の入力履歴のみ、16が選択値のみ、31が全て

			function SetReadOnly()
			{
				bReadOnly = true;
				var cl = document.getElementById("S003BU1000");
				if (cl) cl.disabled = true;
				var cl2 = document.getElementById("S003BU1002");
				if (cl2) cl2.disabled = true;
			}
			//	ドロップダウン選択
			function ArList_Selected(sel)
			{
				var val = sel.options[sel.selectedIndex].value;
				var elText = sel.parentElement.getElementsByTagName("TEXTAREA");
				var selText = elText[0].value;
				var dr = document.getElementById("DefaultRows");

				if (("\r" + selText + "\r").indexOf("\r" + val + "\r") != -1)
				    return false;
								
				if (sel.selectedIndex == 0)
					return false;

				if (_ctrlKey == false) {
	                elText[0].value = "";
	            }
	            AddValue(elText[0], val);
	            CheckRow(elText[0], dr.value);
				//sel.selectedIndex = 0;
				return false;
            }
            function wdcDate_AfterDropDown(oDateChooser, dropDownElement, oEvent) {
                document.body.scrollTop = document.body.scrollHeight;
            }
			//	カレンダー選択
            $('input[type="date"]').live('change', function () {
                var val = $(this).val();
                if (val.length == 0) return;
                val = val.replace(/-/g,'/');
                var $txt = $(this).parent().siblings('textarea');
				var dr = document.getElementById("DefaultRows");
				AddValue($txt.get(0), val);
				CheckRow($txt.get(0), dr.value);
            });
			//	条件設定ボタンクリック
			function cmdSearch_Clicked()
			{
				var p_op = document.getElementById("p_op");
				var p_args = document.getElementById("p_args");
			    var mode = document.getElementById("mode");
				bCondError = false;
				p_args.value = CreateConditionString(true);
				if (!bCondError) {
				    if (mode.value == "edit") {
				        p_op.value = "SetCondition";
				        document.getElementById("frm").submit();
				    } else {
				        if (document.getElementById("S003CB1002").checked == false) {
				            parent.frames["work"].document.getElementById("p_fromvfcond").value = "true";
				        }
				        return DoAttrSearch();
				    }
				    return false;
				} else
				    return false;
            }
            function UpdateEasySearchButtonTid(tid) {
                pw = parent.opener.parent;
                pw.SetEasySearchFolderTid(tid);
            }
            function DoAttrSearch() {
                var p_op = document.getElementById("p_op");
                p_op.value = "Redraw";
                var p_args = document.getElementById("p_args");
                if (p_args.value == "") return false;
                
                parent.g_searching = true;
				if (parent.frames["work"].document.getElementById("frm") != undefined) {
					var ddl = document.getElementById("S003SL1002");
				    switch (ddl.value) {
				        case "AND":
					        parent.frames["work"].document.getElementById("p_op").value = "AttrAndSearch";
					        break;
				        case "OR":
					        parent.frames["work"].document.getElementById("p_op").value = "AttrOrSearch";
					        break;
				    }
				    parent.g_keepconditionpane = true;
				    parent.frames["work"].document.getElementById("p_args").value = p_args.value;
					parent.frames["work"].document.getElementById("frm").submit();
					window.status = "検索実行中...";         //検索実行中...
				} else {
					parent.g_searching = false;
	            }

                // 検索後はFileListのグリッドにフォーカスを置く
                if (window.parent.frames["main"].document.getElementById("gd"))
                    window.parent.frames["main"].document.getElementById("gd").focus();

	            return false;            
            }
			function CreateConditionString(bsubmit)
			{
			    var allowBlank = !bsubmit;
			    var chkOpAsTemplate = document.getElementById("S003CB1002");
			    if (chkOpAsTemplate.checked) allowBlank = true;
			    var dr = document.getElementById("DefaultRows");
			    var al, op, selText, v, c;
			    var n = 0;
				var cond = new Array();				
			    var mode = document.getElementById("mode");
				var pnl = document.getElementById("searcharea");
				var txts = pnl.getElementsByTagName("TEXTAREA");
				for (var i=0;i<txts.length;++i) {
					selText = txts[i].value;
                    if (selText.match(/.*<.+>.*/g)) {
					    ShowAlertDialog('D267', "検索値にタグ(< >)は利用できません。");   //検索値にタグ(< >)は利用できません。
				        bCondError = true;
					    return "";
					}
					if (selText.length == 0 && txts[i].parentElement.className.lastIndexOf("GrFrame") >= 0) {
					    ShowAlertDialog('D268', "ラベルの突き抜け検索では空白は使用できません。");   //ラベルの突き抜け検索では空白は使用できません。
				        bCondError = true;
					    return "";
					}
					v = escape(selText).split("%0A");
					al = $(txts[i]).closest('tr').get(0).getElementsByTagName("A")
					op = $(txts[i]).closest('tr').get(0).getElementsByTagName("SELECT")
					var opval = op[0].options[op[0].selectedIndex].value;
					if (selText.length == 0 && mode.value == "view" && opval != "6") continue;
					var validCond = true;
					switch (opval) {
				    case "1":
				        if (txts[i].value == "" && !allowBlank) validCond = false;
				        break;
				    case "2":
				    	switch (v.length) {
				    		case 1:
				    			v[1] = "";
				    			break;
				    		case 2:
				    			break;
				    		default:
				    			if (v.length > 2) {
				    				if (v[2].length > 0) {
				    					ShowAlertDialog('D269', "$1検索の場合、1行目と2行目の値が検索条件として使用されます。3行目以降の値は消去します。".replace("$1", op[0].options[op[0].selectedIndex].text));    //検索の場合、1行目と2行目の値が検索条件として使用されます。3行目以降の値は消去します。
				    				}
				    				txts[i].value = unescape(v[0] + "\n" + v[1]);
				    			}
				    			break;
				    	}
				    	var bSwap = false;
				    	if (op[0].attributes["abbr"] && op[0].attributes["abbr"].value == "3") {
				    		if (unescape(v[0]) - 0 > unescape(v[1]) - 0) bSwap = true;
				    	} else {
				    		var v0 = ConvVarDateValue(unescape(v[0]));
				    		var v1 = ConvVarDateValue(unescape(v[1]));
				    		if (v0 != null && v1 != null) {
				    			if (MyDateDiff("d", v0, v1) < 0) bSwap = true;
				    		} else {
				    			if (zeropad(unescape(v[0])) > zeropad(unescape(v[1]))) bSwap = true;
				    		}
				    	}
				    	if (bSwap && v[0].length > 0 && v[1].length > 0) {
				    		var t = v[0];
				    		v[0] = v[1];
				    		v[1] = t;
				    		txts[i].value = unescape(v[0] + "\n" + v[1]);
				    	}
				    	break;
					case "3":
						if (v.length > 1) {
							if (v[1].length > 0) {
								ShowAlertDialog('D270', "$1検索の場合、1行目の値が検索条件として使用されます。2行目以降の値は消去します。".replace("$1", op[0].options[op[0].selectedIndex].text));          //検索の場合、1行目の値が検索条件として使用されます。2行目以降の値は消去します。
								bCondError = true;
							}
						}
						txts[i].value = unescape(v[0]);
						if (txts[i].value == "" && !allowBlank) validCond = false;
						break;
					case "4":
						if (v.length > 1) {
							if (v[1].length > 0) {
							    ShowAlertDialog('D271', "$1検索の場合、1行目の値が検索条件として使用されます。2行目以降の値は消去します。".replace("$1", op[0].options[op[0].selectedIndex].text));          //検索の場合、1行目の値が検索条件として使用されます。2行目以降の値は消去します。
								bCondError = true;
							}
						}
						txts[i].value = unescape(v[0]);
						if (txts[i].value == "" && !allowBlank) validCond = false;
						break;
		            case "6":
		                txts[i].value = "";
		                break;
		            default:
						break;
					}
					CheckRow(txts[i], dr.value);

					if (!validCond) continue;
					
					selText = txts[i].value;
					v = escape(selText).split("%0A");
					c = al[0].innerText + "x" + opval + "x" + unescape(v.join("\t"));
					if (!bsubmit) {
						cond[n++] = "c"+c;
					} else {
						cond[n++] = c;
					}
				}
				return cond.join("\n");
			}
			//	検索条件をクリア
			function cmdClear_Click()
			{
			    var p_op = document.getElementById("p_op");
				var p_args = document.getElementById("p_args");
				p_op.value = "Redraw";
				p_args.value = "";
				document.getElementById("frm").submit();
				return false;
			}
			function body_ondragenter(event)
			{
                event.returnValue = false;
                event.preventDefault();
			}
			function body_ondragover(event)
			{
				event.returnValue = false;
                event.preventDefault();
			}
			function body_ondrop(event)
			{
				var newValue = event.dataTransfer.getData("Text");
				AddCondition(newValue);
			}
			function AddCondition(newValue)
			{
				if (bReadOnly) {
					ShowAlertDialog('D272', "読み込み権の無い検索条件は変更できません。");  //読み込み権の無い検索条件は変更できません。
					return;
				}
			    var cnt = $('#p_condCount').val();
			    var limit = $('#p_limitCondCount').val();
			    if (parseInt(cnt) >= parseInt(limit)) {
			        //  既に30個検索条件が設定されている
			        ShowAlertDialog('D306', 'これ以上検索条件を設定することはできません。');   //これ以上検索条件を設定することはできません
			        return;
			    }
			    var cond;
				var cur = CreateConditionString(false);
				if (cur == "") {
					cond = newValue;
				} else {
					cond = cur + "\n" + newValue;
				}
				var p_op = document.getElementById("p_op");
				var p_args = document.getElementById("p_args");
				p_op.value = "Redraw";
				p_args.value = cond;
				document.getElementById("frm").submit();
				return false;
            }
            function ClearConditionValue() {
                var name;
                var pnl = document.getElementById("searcharea");
                var txts = pnl.getElementsByTagName("TEXTAREA");
                var dr = document.getElementById("DefaultRows");
                for (var i = 0; i < txts.length; ++i) {
                    if (txts[i].readOnly) {
                        continue;
                    }
                    txts[i].value = "";
                    name = txts[i].name;
                    if (name.lastIndexOf("txtValues") == -1)
                        txts[i].rows = 1;
                    else
                        txts[i].rows = dr.value;
                }
                var ops = pnl.getElementsByTagName("SELECT");
                for (var i = 0; i < ops.length; ++i) {
                    if (ops[i].attributes["abbr"])
                        ops[i].selectedIndex = ops[i].attributes["abbr"].value - 0;
                    else
                        ops[i].selectedIndex = 0;
                }
                var inps = pnl.getElementsByTagName("INPUT");
                for (var i = 0; i < inps.length; ++i) {
                    if ((!inps[i].readOnly) && (!inps[i].isMultiLine))
                        inps[i].value = "";
                }
                window.status = "";
                return false;
            }
            function RemoveCondition(e) {
                var tr = e.parentElement.parentElement;
                var cnt = tr.parentElement.childNodes.length;
                tr.parentElement.removeChild(tr);
                $('#p_condCount').val(cnt-1);
            }
