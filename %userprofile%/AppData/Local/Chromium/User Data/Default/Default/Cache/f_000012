			    parent.opener.parent.frames["contents"].DoSearch(null, false);
			}
            function ShowHideConditionPane() {
                var btnShowHide = document.getElementById("btnShowHide");
                if (btnShowHide.src.indexOf("desc.gif") < 0) {
                    document.getElementById("conditions").style.display = "none";
                    btnShowHide.src = "i/desc.gif";
                    parent.SetConditionVisible(true, false, false);
                    document.getElementById("S003BU1000").style.display = "none";
                } else {
                    document.getElementById("conditions").style.display = "";
                    btnShowHide.src = "i/asc.gif";
                    parent.SetConditionVisible(true, true, false);
                    document.getElementById("S003BU1000").style.display = "block";
                }
            }
            function ToggleConditionPaneVisible() {
                var btnShowHide = document.getElementById("btnShowHide");
                var visible = true;
                if (btnShowHide.src.indexOf("desc.gif") < 0) {
                    visible = false;
                }
                ShowHideConditionPane();
                _busy = true;
                $.post('fwajaxproc.aspx', {
                    'cmd': 'SetConditionPaneVisible',
                    'visible': visible
                }, function () {
                    _busy = false;
                });
            }
            function SetConditionPaneVisible(visible) {
                if (visible == "true")
                    parent.SetConditionVisible(true, true, true);
                else
                    parent.SetConditionVisible(true, false, true);
            }
            function SetControlVisible(visible) {
                try {
                    switch (visible) {
                        case true:
                            document.getElementById("conditions").style.visibility = "visible";
                            break;
                        case false:
                            document.getElementById("conditions").style.visibility = "hidden";
                            break;
                    }
                } catch (e) { }
            }
			//	デフォルトのポップアップメニューを非表示にする
            document.oncontextmenu = function () {
                switch (event.srcElement.tagName.toUpperCase()) {
                    case "TEXTAREA":
                    case "INPUT":
                        return true;
                    default:
                }
                return false;
            }
            document.onkeydown = function ()
		    {
                _ctrlKey = event.ctrlKey;
			    switch (event.keyCode) {
		        case 116: //	F5
		            event.returnValue = false;
		            event.cancelBubble = true;
		            event.keyCode = 0;
		            return;
                }
            }
		    document.onkeyup = function() {
                _ctrlKey = event.ctrlKey;
                switch (event.keyCode) {
                    case 13:    //  Enter
                        if (event.ctrlKey && $("#p_locked").val() != "true") {
                            cmdSearch_Clicked();
                            return;
                        }
                        // not break ↓へ
                    case 8:     //  BackSpace
                    case 46:    //  DEL
                        var el = document.activeElement;
                        if (el.tagName.toLowerCase() != "textarea") return;
                        var dr = document.getElementById("DefaultRows");
                        CheckRow(el, dr.value);
                        break;
                }
            }
            function onDragging() {
                var w = window.parent;
                w.onDragging(event.clientX, "rightPane");
            }
            //	%DATE○○%の書式を日付文字列に変換して返す。もし所定の書式でない場合null
            function ConvVarDateValue(value) {
            	var pos;
            	var strInterval;
            	if (value.indexOf('%DATE') == 0) {
            		pos = value.indexOf("%", 5);
            		if (pos == 5) {
            			var d = new Date();
            			result = d.getYear().toString() + "/" + (d.getMonth() + 1).toString() + "/" + d.getDate();
            			return result;
                    } else if (pos > 5) {
            			var ss = value.substr(5, 1);
            			if (ss == "y" || ss == "m" || ss == "d" || ss == "w") {
            				if (ss == "y") {
            					strInterval = "yyyy";
            				} else if (ss == "w") {
            					strInterval = "w";
            				} else {
            					strInterval = ss;
            				}
            				var num = value.substring(6, pos);
            				if (!num || num == "") num = 0;
            				var d = new Date();
            				var cd = d.getYear().toString() + "/" + (d.getMonth() + 1).toString() + "/" + d.getDate();
            				try {
            					var result = MyDateAdd(strInterval, num, cd);
            					return result;
            				} catch (e) {
            				}
            			}
            		}
            	}
            	return null;
         }
         //	日付の0埋めをする
         function zeropad(value) {
         	var s = value.split("/");
         	if (s.length == 3) {
         		var ds = ("0000" + s[0]).slice(-4) + "/" + ("00" + s[1]).slice(-2) + "/" + ("00" + s[2]).slice(-2);
         		return ds;
         	}
         	return value;
         }
         //AutoCompleteを設定する
         function setupAutoComplete4Str(elem, aaid) {
             _busy = true;
             var list = eval($('#suggestSource4Str').text());
             $("#" + elem.id).autocomplete({
                 source: function (request, response) {
                     var filteredArray = $.map(list, function (item) {
                         if (item.value.toLowerCase().indexOf(request.term.toLowerCase()) == 0) {
                             return item;
                         }
                         else {
                             return null;
                         }
                     });
                     response(filteredArray);
                 },
                 autoFocus: true,
                 minLength: 1,
                 position: { my: "right top", at: "right bottom" },
                 delay: 200
             });
         }
         function setupAutoComplete4Date(elem, aaid) {
             _busy = true;
             var list = eval($('#suggestSource4Date').text());
             $("#" + elem.id).autocomplete({
                 source: function (request, response) {
                     var filteredArray = $.map(list, function (item) {
                         if (item.value.toLowerCase().indexOf(request.term.toLowerCase()) == 0) {
                             return item;
                         }
                         else {
                             return null;
                         }
                     });
                     response(filteredArray);
                 },
                 autoFocus: true,
                 minLength: 1,
                 position: { my: "right top", at: "right bottom" },
                 delay: 200
             });
         }
         function setupAutocomplete(elem, aaid) {
             _busy = true;
             $("#" + elem.id).autocomplete({
                 source:
                 function (request, response) {
                     $.ajax({
                         url: "fwajaxproc.aspx",
                         type: 'POST',
                         async: 'true',
                         data: {
                             'cmd': 'GetTermList',
                             'term': request.term,
                             'aaid': aaid,
                             'type': termType
                         },
                         dataType: "json",
                         success: function (data) {
                             _busy = false;
                             response(data);
                         },
                         error: function () {
                             _busy = false;
                         }
                     });
                 },
                 minLength: 0,
                 delay: 200
             });
             $("#" + elem.id).focus(
                 function () {
                     termType = 1;
                     $(this).autocomplete("search", "");
             });
             $("#" + elem.id).keydown(
                 function (e) {
                    _ctrlKey = e.ctrlKey;
                     if (e.keyCode != 39) { //39 右カーソル
                         if (termType == 1) termType = 31;   //  直近5個から全てへモード変更
                         return;
                     }
                     var range = document.selection.createRange();
                     range.moveStart("character", -elem.value.length);
                     if (range.text.length != elem.value.length) {
                         return;
                     }
                     //  最後の位置で右カーソルを押した時だけ反応
                     if (termType == 16) {
                         termType = 31;  //全て
                     } else {
                         termType = 16;   //選択値のみ
                     }
                     $(this).autocomplete("search", $(this).val());
                     return false;
                 });
         }
         //  語句候補をサーバ側に渡す
         function setTermHistory(elem, aaid) {
             _busy = true;
             var term = $("#" + elem.id).val();
             $.ajax({
                 url: "fwajaxproc.aspx",
                 type: 'POST',
                 async: 'true',
                 data: {
                     'cmd': 'SetTermHistory',
                     'term': term,
                     'aaid': aaid
                 },
                 success: function (data) {
                     _busy = false;
                 },
                 error: function () {
                     _busy = false;
                 }
             });
         }
         $(function () {
             var chks = ["i/cb_x.gif", "i/cb_0.gif", "i/cb_1.gif"]
             $('.checkCond').on('click', function () {
                 var v = $(this).attr('data-val');
                 v--;
                 if (v < -1) v = 1;
                 $('img', $(this)).attr('src', chks[v + 1]);
                 $(this).attr('data-val', v);
                 var $txt = $(this).prev('textarea');
                 switch (v) {
                     case 0:
                         $txt.val('0');
                         break;
                     case 1:
                         $txt.val('1');
                         break;
                     default:
                         $txt.val('');
                         break;
                 }
             });
            window.addEventListener('keydown', function (e) {
                _ctrlKey = e.ctrlKey;
            }, true);
            window.addEventListener('keyup', function (e) {
                _ctrlKey = e.ctrlKey;
            }, true);
            $(document).on("keydown", $('select'), function (e) {
                _ctrlKey = e.ctrlKey;
            });
            $(document).on("keyup", $('select'), function (e) {
                _ctrlKey = e.ctrlKey;
            });
             $('[data-date]').datepicker({
                 showOn: 'button',
                 buttonImage: 'i/dpbtn2.png',
                 buttonImageOnly: true,
                 changeMonth: true,
                 changeYear: true,
                 showButtonPanel: true,
                 dateFormat: 'yy/mm/dd',
                 showAnim: '',
                 onSelect: function (val, cal) {
                     if (val.length == 0) return;
                     val = val.replace(/-/g,'/');
                     var $txt = $(this).parent().siblings('textarea');
				     var dr = document.getElementById("DefaultRows");
				     AddValue($txt.get(0), val);
				     CheckRow($txt.get(0), dr.value);
                }
            });
            $('.ui-datepicker-current').live('click', function (e) {
                $('a', '.ui-datepicker-today').click();
            });
             $(window).on('resize', function () {
                 $('#searcharea').height($(window).height() - $('#searcharea').offset().top);
             }).resize();
         });
		</script>

