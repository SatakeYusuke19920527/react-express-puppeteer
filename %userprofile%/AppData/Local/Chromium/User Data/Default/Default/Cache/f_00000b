			function wt_NodeCollapse(treeId, nodeId){
				// ノードを閉じる場合は Postback しない
				igtree_cancelPostBack("wt");
				
				var nd = igtree_getNodeById(nodeId);
				var path = GetNodeFullPath(nd);
				
				// 検索中ならばキャンセル
				if (parent.g_searching && path != fullpath) {
					return true;
				}

			}
			function wt_NodeExpand(treeId, nodeId){
				// 検索中ならばキャンセル
				var nd = igtree_getNodeById(nodeId);
				if (parent.g_searching) {
					return true;
				}
				
				//	サブノードが存在していれば Postback しない
				var nd = igtree_getNodeById(nodeId);
				var ts=igtree_treeState["wt"];
				if(nd.getFirstChild() != null || ts.LoadOnDemand>=1) {
				    igtree_cancelPostBack("wt");
				    m_expandNodeId = nodeId;
				    setTimeout(getFolderCount, 1);
					return;
				}
			}
			//	名前変更完了
			function wt_AfterEndNodeEdit(treeId, nodeId){
				var nd = igtree_getNodeById(nodeId);
				var newName = nd.getText();
				var cn = newName.replace(/^[\s　]+|[\s　]+$/g, '');
				if (cn.length == 0) {
					var cpath = parent.g_currentpath;
					var ot = GetOriginalName(nd);
					if (
	                (cpath.indexOf("よく使う検索") == 0)
	                || (cpath.indexOf("共有よく使う検索") == 0)   // 個人バーチャル||共有バーチャル
	                || (cpath.indexOf("マーク") == 0)
	                || (cpath.indexOf("共有マーク") == 0)) {   // 個人ワーク||個人バーチャル
						//よく使うかマークフォルダ。元に戻す
						nd.setText(ot);
					} else {
						//分類フォルダ。
						if (ot.replace(/^[\s　]+|[\s　]+$/g, '').length == 0) {
							//	元が空白。つまり新規ノードだった
							if (event.keyCode == 27) {
								// ESCで来たのでノード削除（もとに戻す）
								var ts = igtree_getTreeById("wt");
								var pat = nd.getParent();
								nd.remove();
								ts.setSelectedNode(pat);
								return false;
							} else {
								// Enterなどで来た。警告して編集モード継続（119350）
                                parent.ShowAlertDialog('D138', "フォルダ名が空白です。", "フォルダ名入力文字の確認",'alert',
                                    function (returnValue, args) {
				                        nd.setText("&nbsp;");
				                        StartNodeEdit();
                                    }
                                );  //フォルダ名が空白です
								return true;
							}

						} else {
							//	元に戻す
							nd.setText(ot);
						}
				    }
				    igtree_cancelPostBack("wt");
				    return;
				}

				if (GetOriginalName(nd) == newName) {
				    igtree_cancelPostBack("wt");
				    return;
				} else if ((newName.indexOf("\\") > -1) ||
					(newName.indexOf("/") > -1) ||
					(newName.indexOf(":") > -1) ||
					(newName.indexOf("*") > -1) ||
					(newName.indexOf("?") > -1) ||
					(newName.indexOf("\"") > -1) ||
					(newName.indexOf("&gt;") > -1) ||
					(newName.indexOf("<") > -1) ||
					(newName.indexOf("&lt;") > -1) ||
					(newName.indexOf(">") > -1) ||
                    (newName.indexOf("|") > -1)) {
				    igtree_cancelPostBack("wt");
                    parent.ShowAlertDialog('D138', "フォルダ名に次の文字は使えません\n \\/:*?\"<>|", "フォルダ名入力文字の確認",'alert',
                        function (returnValue, args) {
				            var ot = newName.replace(/</g, "&lt;").replace(/>/g, "&gt;");
				            nd.setText(ot);
				            StartNodeEdit();
                        }
                    );  //フォルダ名に次の文字は使えません,フォルダ名入力文字の確認
					return;
				}
            }
            function afterFolderNameIsSpace(returnValue, args) {
            }
            function afterInvalidFolderNameChar(returnValue, args) {
            }
			function StartNodeEdit()
			{
				var ts = igtree_getTreeById("wt");
				var nd = ts.getSelectedNode();
				if (nd == null)
					return true;
				if (nd.getParent() == null)
					return true;
				
			    //  JAXA対応。件数のテキストは削除
				var badgeElem = $("#" + nd.element.id + " span[class=badge]");
				if (badgeElem.get(0)) {
				    badgeElem.remove();
				}

			    //	ラベルエディット
				fullpath = GetNodeFullPath(nd);
                igtree_beginedit(nd.getTreeId(), nd.element.id);
			}
			function GetSendPath()
			{
				// コピー元のパスを取得する　(その他)フォルダのときはパスを渡さない
				if ((fullpath.indexOf("\\(その他)\\") > 0) || (fullpath.indexOf("\\(その他)") > 0)){    //(その他),(その他)
					return "";
				}
				else {
					return fullpath;
				}
            }

            function cnfw_MenuHandler2(key, resid) {
				var ts = igtree_getTreeById("wt");
				var nd = ts.getSelectedNode();
                switch (key) {
					case "mnuMoveUp":	//上へ移動
                        if (!CanExecute(nd, "mnuMoveUp")) return true;
						break;

					case "mnuMoveDown": //下へ移動
						if (!CanExecute(nd, "mnuMoveDown")) return true;
						break;

					case "mnuCreateChildFolder":	//新規作成（下層）
                        if (!CanExecute(nd, "mnuCreateChildFolder")) return true;
						break;

					case "mnuCreateSiblingFolder": //新規作成（同層）
						if (!CanExecute(nd, "mnuCreateSiblingFolder")) return true;
						break;

					case "mnuViewCondition":    //検索条件表示
						SelectedFolderOp("EditCondition", "");
						return true;
						break;

					case "mnuRename":    //名前変更
						if (getTreeType() == 1) {
							if (!CanExecute(nd, "mnuRename")) return true;
						}
						StartNodeEdit();
						return true;
						break;

					case "mnuCreateFolder":    //新規作成
						break;

					case "mnuDeleteFolder":    //フォルダ削除
						if (bfolderdeleting) return true;
						if (getTreeType() == 1) {
							if (!CanExecute(nd, "mnuDeleteFolder")) return true;
                        }
                        $('#p_cmdResid').val(resid);
                        parent.cnfwConfirm('D124', "選択フォルダを削除します。\nよろしいですか？", "フォルダ削除の確認", 3, null, null, deleteFolder);//選択フォルダを削除します。よろしいですか？,フォルダ削除の確認
                        return;
						break;
					case "mnuCut": //切り取り
					    var sendpath = GetSendPath();
						window.parent.document.getElementById("p_CopyFolderPath").value = sendpath;
						window.parent.document.getElementById("p_CopySubFolder").value = true;

						window.parent.document.getElementById("p_Cut").value = nd.Id;
						return true;
						break;
							
					case "mnuCopy":    //コピー
					    var sendpath = GetSendPath();
						if (getTreeType() == 1) {
							if (!CanExecute(nd, "mnuCopy")) return true;
						}
						window.parent.document.getElementById("p_CopyFolderPath").value = sendpath;
						window.parent.document.getElementById("p_CopySubFolder").value = false;
						window.parent.document.getElementById("p_Cut").value = "";
						return true;
						break;

					case "mnuCopyAll":    //コピー(サブフォルダを含む)
					    var sendpath = GetSendPath();
						if (getTreeType() == 1) {
							if (!CanExecute(nd, "mnuCopyAll")) return true;
						}
						window.parent.document.getElementById("p_CopyFolderPath").value = sendpath;
						window.parent.document.getElementById("p_CopySubFolder").value = true;
						window.parent.document.getElementById("p_Cut").value = "";
						return true;
						break;

                    case "mnuPaste":    //貼り付け
                        document.getElementById("p_CopyFolderPath").value = window.parent.document.getElementById("p_CopyFolderPath").value;
                        document.getElementById("p_CopySubFolder").value = window.parent.document.getElementById("p_CopySubFolder").value;
                        document.getElementById("p_Cut").value = window.parent.document.getElementById("p_Cut").value;
						// サブフォルダもコピーする場合、確認をする
						if (document.getElementById("p_CopySubFolder").value == "true") {
							var doAlert;
							if (document.getElementById("p_Cut").value == "") {
								//コピー・貼り付け
								doAlert = true;
							} else {
								//切り取り・貼り付け
								var isOrgFolderShared = checkSharedFolder(document.getElementById("p_CopyFolderPath").value);
								var ts = igtree_getTreeById("wt")
								var nd = ts.getSelectedNode();
								var path = GetNodeFullPath(nd);
								var toFolderShared = checkSharedFolder(path);
								if (isOrgFolderShared != toFolderShared) {
									//	貼り付け元と貼り付け先で、共有・個人の違いがある
									doAlert = true;
								}
							}
							if (doAlert) {
                                if (ShowConfirmDialog("D293", "深い階層のフォルダをコピーすると、かなり時間がかかる場合があります。\nよろしいですか？", "", 1)) {    //深い階層のフォルダをコピーすると、かなり時間がかかる場合があります。よろしいですか。
                                    // postback
                                } else
									return true;
							}
						}
						else {
							// postback
						}
						break;

					case "mnuPermission":    //ファイル登録時の権限設定
						SelectedFolderOp("SetFolderPermission", "");
						return true;
						break;

					case "mnuFolderPermission":    //フォルダに対する権限設定
						SelectedFolderOp("SetFolderOperationPermission", "");
						return true;
						break;

					case "mnuCreateNotice":    //メール通知 新規
						if (nd != null) ShowMailSetting(GetNodeFullPath(nd));
						else parent.ShowAlertDialog('D216', "フォルダを選択してください");   //フォルダを選択してください！

						return true;
						break;

					case "mnuMailSettings":    //メール通知 一覧
						if (nd != null) ShowMailList(GetNodeFullPath(nd));
						else parent.ShowAlertDialog('D217', "フォルダを選択してください");   //フォルダを選択してください！

						return true;
						break;

					case "mnuGetFiles":  //取り出し
						DownloadFolder();
						return true;
						break;

					case "mnuProperty": //プロパティ
					    document.getElementById("p_selectedNdid").value = nd.Id;
						var w = window.open("fdrprop.aspx?tid=" + 0, "_blank", "width=640,height=280,dependent=yes", true);
						if (w == undefined)
							return;
						var d = new Date();
						window.parent.AddWindow(w, "fdrpropwin" + d.toUTCString());
						return true;
						break;

					default:
						break;
                }
                ExecuteCommandEx(key, resid);

            }
            function deleteFolder(returnValue, args) {
			    if (returnValue) {     
				    bfolderdeleting = true;
                    ExecuteCommandEx("mnuDeleteFolder", $('#p_cmdResid').val());
			    } else
				    return true;
            }
            //  ポストバックによるサーバ処理
            function ExecuteCommandEx(cmd, resid) {
                $('#p_cmd').val(cmd);
                $('#p_cmdResid').val(resid);
                $('#frmTree').submit();
            }

			//	ポップアップメニューのクリックイベント
			function mnuPopup_ItemClick(menuId, itemId) {
				return WebMenuClick(menuId, itemId);
			}
			//	ポップアップメニューのクリックイベント
			function mnuPopup_ItemChecked(menuId, itemId, bChecked) {
				return WebMenuClick(menuId, itemId, bChecked);
			}

			//  メニューのクリック処理
			function WebMenuClick(menuId, itemId, bChecked) {
				var mnu = igmenu_getItemById(itemId);
				var x = igmenu_getLeftPos(mnu.Element);
				var tag = mnu.getTag();
				var s = tag.split(",");
				var resid = s[1];
				if (!resid) {
					//IDがセットされていないので何もしない。
					return true;
				}
				tag = s[0];
				if (m_isGUIEditMode && window.event != null && (window.event.clientX - 24 <= x)) {
					//GUI編集モードで、左端のチェックの位置をクリックした
					mnu.setChecked(!mnu.getChecked()); //    フラグの反転
					//  表示・非表示設定をAjax経由で呼び出し
					var enabled;
					if (mnu.getChecked()) {
					    enabled = "true";
					} else {
					    enabled = "false";
						}
					try {
					    $.ajax({
					        url: "configmode.aspx",
					        type: 'POST',
					        async: false,
					        data: {
					            'setenabled': enabled,
					            'resid': resid
					        },
					        dataType: "text",
					        success: function (ret) {
					        },
					        error: function () {
					}
					    });
					} catch (e) { }

					return true; //POST backしない                
				} else {
					if (m_isGUIEditMode && !mnu.getChecked()) {
						//  無効化されているコマンドなので何もしない
						return true;
					}
					//	選択アイテムとそのキャプションを取得
					var item = igmenu_getItemById(itemId);
					var cmd = item.Element.innerText.replace("<U>", "").replace("</U>", "");
					var work_frm = parent.frames["work"].document;

					// 先にメニューを非表示にする
					cnfw_igmenu_hideMenu(itemId);

					var sendpath = GetSendPath();

					var ts = igtree_getTreeById("wt");
					var nd = ts.getSelectedNode();
					// メニューの実行。return false の場合、Postback で処理する。
					switch (tag) {
						case "mnuMoveUp":	//上へ移動
							var b = CanExecute(nd, "mnuMoveUp");
							return !b;	//	trueでpostbackしなくなるので反転
							break;

						case "mnuMoveDown": //下へ移動
							var b = CanExecute(nd, "mnuMoveDown");
							return !b; //	trueでpostbackしなくなるので反転
							break;

						case "mnuCreateChildFolder":	//新規作成（下層）
							var b = CanExecute(nd, "mnuCreateChildFolder");
							return !b; //	trueでpostbackしなくなるので反転
							break;

						case "mnuCreateSiblingFolder": //新規作成（同層）
							var b = CanExecute(nd, "mnuCreateSiblingFolder");
							return !b; //	trueでpostbackしなくなるので反転
							break;

						case "mnuViewCondition":    //検索条件表示
							SelectedFolderOp("EditCondition", "");
							return true;
							break;

						case "mnuRename":    //名前変更
							if (getTreeType() == 1) {
								var b = CanExecute(nd, "mnuRename");
								if (b == false) {
									return true;
									break;
								}
							}
							StartNodeEdit();
							return true;
							break;

						case "mnuCreateFolder":    //新規作成
							cnfw_ShowFiles("");
							return false;
							break;

						case "mnuDeleteFolder":    //フォルダ削除
							if (bfolderdeleting) return true;
							if (getTreeType() == 1) {
								var b = CanExecute(nd, "mnuDeleteFolder");
								if (b == false) {
									return true;
									break;
								}
							}
							if (ShowConfirmDialog('D124', "選択フォルダを削除します。\nよろしいですか？", "フォルダ削除の確認", 3)) {     //選択フォルダを削除します。よろしいですか？,フォルダ削除の確認
								bfolderdeleting = true;
								return false; // postback
							} else
								return true;
							break;
						case "mnuCut": //切り取り
							window.parent.document.getElementById("p_CopyFolderPath").value = sendpath;
							window.parent.document.getElementById("p_CopySubFolder").value = true;

							window.parent.document.getElementById("p_Cut").value = nd.Id;
							return true;
							break;
							
						case "mnuCopy":    //コピー
							if (getTreeType() == 1) {
								var b = CanExecute(nd, "mnuCopy");
								if (b == false) {
									return true;
									break;
								}
							}
							window.parent.document.getElementById("p_CopyFolderPath").value = sendpath;
							window.parent.document.getElementById("p_CopySubFolder").value = false;
							window.parent.document.getElementById("p_Cut").value = "";
							return true;
							break;

						case "mnuCopyAll":    //コピー(サブフォルダを含む)
							if (getTreeType() == 1) {
								var b = CanExecute(nd, "mnuCopyAll");
								if (b == false) {
									return true;
									break;
								}
							}
							window.parent.document.getElementById("p_CopyFolderPath").value = sendpath;
							window.parent.document.getElementById("p_CopySubFolder").value = true;
							window.parent.document.getElementById("p_Cut").value = "";
							return true;
							break;

						case "mnuPaste":    //貼り付け
							document.getElementById("p_CopyFolderPath").value = window.parent.document.getElementById("p_CopyFolderPath").value;
							document.getElementById("p_CopySubFolder").value = window.parent.document.getElementById("p_CopySubFolder").value;
							document.getElementById("p_Cut").value = window.parent.document.getElementById("p_Cut").value;
							// サブフォルダもコピーする場合、確認をする
							if (document.getElementById("p_CopySubFolder").value == "true") {
								var doAlert;
								if (document.getElementById("p_Cut").value == "") {
									//コピー・貼り付け
									doAlert = true;
								} else {
									//切り取り・貼り付け
									var isOrgFolderShared = checkSharedFolder(document.getElementById("p_CopyFolderPath").value);
									var ts = igtree_getTreeById("wt")
									var nd = ts.getSelectedNode();
									var path = GetNodeFullPath(nd);
									var toFolderShared = checkSharedFolder(path);
									if (isOrgFolderShared != toFolderShared) {
										//	貼り付け元と貼り付け先で、共有・個人の違いがある
										doAlert = true;
									}
								}
								if (doAlert) {
									if (ShowConfirmDialog("D293", "深い階層のフォルダをコピーすると、かなり時間がかかる場合があります。\nよろしいですか？", "", 1))     //深い階層のフォルダをコピーすると、かなり時間がかかる場合があります。よろしいですか。
										return false; // postback
									else
										return true;
								}
							}
							else {
								return false; // postback
							}
							break;

						case "mnuPermission":    //ファイル登録時の権限設定
							SelectedFolderOp("SetFolderPermission", "");
							return true;
							break;

						case "mnuFolderPermission":    //フォルダに対する権限設定
							SelectedFolderOp("SetFolderOperationPermission", "");
							return true;
							break;

						case "mnuCreateNotice":    //メール通知 新規
							var ts = igtree_getTreeById("wt")
							var nd = ts.getSelectedNode();

							if (nd != null) ShowMailSetting(GetNodeFullPath(nd));
							else parent.ShowAlertDialog('D216', "フォルダを選択してください");   //フォルダを選択してください！

							return true;
							break;

						case "mnuMailSettings":    //メール通知 一覧
							var ts = igtree_getTreeById("wt")
							var nd = ts.getSelectedNode();

							if (nd != null) ShowMailList(GetNodeFullPath(nd));
							else parent.ShowAlertDialog('D217', "フォルダを選択してください");   //フォルダを選択してください！

							return true;
							break;

						case "mnuGetFiles":  //取り出し
							DownloadFolder();
							return true;
							break;

					    case "mnuProperty": //プロパティ
					        var nd = ts.getSelectedNode();
					        document.getElementById("p_selectedNdid").value = nd.Id;
							var w = window.open("fdrprop.aspx?tid=" + 0, "_blank", "width=640,height=280,dependent=yes", true);
							if (w == undefined)
								return;
							var d = new Date();
							window.parent.AddWindow(w, "fdrpropwin" + d.toUTCString());
							return true;
							break;

						default:
							return false; // postback
							break;
					}
				}
			}
			function Trim(str)
			{
			    return str.replace(/^\s+|\s+$/g, "");
			}
			function IsEnableEditSelectedNode() {
				var fullpath = UpdateCurrentPath();
				if (fullpath == "") return false;
                var node = fullpath.split("\\");
				var vflock = window.document.getElementById("p_KYOUYU_vf_EditOff").defaultValue;
                if (node.length < 2) return false;
                switch (node[0]) {
                case "共有よく使う検索":
                    if (vflock == "1") return false;
                    break;
                case "よく使う検索":
                    break;
                case "共有マーク":
                    break;
                case "マーク":
                    if (node.length == 2 && node[1] == "Work Area") return false;
                    break;
                default:
                    return true;
                    break;
                }
                return true;
            }
			//	コマンドが実行可能かをチェックし不可なら警告を出したうえでfalseを返す
            function CanExecute(nd, command) {
            	var result = true;
				switch (command) {
					case "mnuMoveUp":	//上へ移動
						if (nd.getParent() == null) {
							//	トップフォルダでは上下移動なし
							result = false;
							break;
						}
						result = checkEnableMenu(nd, "move");
						if (nd.getIndex() == 0) {
							result = false;
						}
						break;
					case "mnuMoveDown":	//下へ移動
						if (nd.getParent() == null) {
							//	トップフォルダでは上下移動なし
							result = false;
							break;
						}
						result = checkEnableMenu(nd, "move");
						var nnd = nd.getNextSibling();
						if (nnd == null || GetOriginalName(nnd) == "(その他)" || GetOriginalName(nnd) == "(選択しない)") {
							result = false;
						}
						break;
					case "mnuCreateChildFolder":	//新規作成（下層）
						result = checkEnableMenu(nd, "createfolder");
						break;
					case "mnuCreateSiblingFolder":	//新規作成（同層）
						if (nd.getParent() == null) {
							//	トップフォルダでは新規作成（同層）なし
							result = false;
							break;
						}
						result = checkEnableMenu(nd, "createfolder");
						break;
					case "mnuRename":
						result = checkEnableMenu(nd, "rename");
						break;
					case "mnuDeleteFolder":
						result = checkEnableMenu(nd, "delete");
						if (bfolderdeleting) result = false;
						break;
					case "mnuCopy":
						if (nd.getParent() == null) {
							result = false;
						}
						break;
					case "mnuCopyAll":
						if (nd.getParent() == null) {
							result = false;
						}
						break;
					default:
						break;
				}
				if (result == false) {
					parent.ShowAlertDialog('D287', "このフォルダでは実行できないコマンドです。");
				}
				return result;
            }
            function setMenuEnabled(nd) {
				//分類フォルダは常に表示
				if (getTreeType() == 1) {
					return true;
                }
				
				// 選択フォルダの文字列を調べる
				var ParantNdFlg = false;
				var ndTxt = "";
				if ((nd != null) && (nd != undefined)) {
					ndTxt = nd.Element.innerText;
					
					// 親フォルダかどうか調べる
					var path = GetNodeFullPath(nd);
					if (Trim(ndTxt) == Trim(path))
						ParantNdFlg = true;
				}
				
				// 共有フォルダの編集が不可になっているかどうかチェック
				var strKYOUYU_vf_Lock = window.document.getElementById("p_KYOUYU_vf_EditOff").defaultValue;
				var strPathNods = fullpath.split("\\");
				var isSharedFolder = false;
				var EditLock = false;
				var HideDel = false;
				if (strPathNods[0] == "共有よく使う検索") {	//共有バーチャル
					isSharedFolder = true;
				    if (strKYOUYU_vf_Lock == "1")
				    	EditLock = true;
				    if (ro)
				        HideDel = true;
                }
				if (strPathNods[0] == "共有マーク") {	//共有マーク	
					isSharedFolder = true;
					if (ro)
				        HideDel = true;
				}

                $('a[data-key]', '#mnuPopup').each(function () {
                    var $item = $(this).closest('li');
                    var key = $(this).attr('data-key');
                    switch (key) {
                        case "mnuFolderPermission":    //	フォルダに対する権限設定
							if (isSharedFolder) {
                                if (EditLock || HideDel) $item.addClass('ui-state-disabled');// item.setEnabled(false);
								else $item.removeClass('ui-state-disabled');// item.setEnabled(true);
							} else {
								$item.addClass('ui-state-disabled');// item.setEnabled(false);
							}
                            break;
                        case "mnuPaste":    //貼り付け
							// 貼り付け元が指定されていないときは使用できないようにする
							if (window.parent.document.getElementById("p_CopyFolderPath").value == "") {
								$item.addClass('ui-state-disabled');// item.setEnabled(false);
							}
							else if (EditLock || HideDel) {
								$item.addClass('ui-state-disabled');// item.setEnabled(false);
							}
							else {
								$item.removeClass('ui-state-disabled');// item.setEnabled(true);
							}
                            break;
                        case "mnuMoveUp":    //上へ移動
                            break;
                        case "mnuMoveDown":    //下へ移動
                            break;
                        case "mnuCreateChildFolder":    //新規作成（下層）
                            break;
                        case "mnuCreateSiblingFolder":    //新規作成（同層）
                            break;
                        case "mnuCreateFolder":    //新規作成
							if (EditLock || HideDel) $item.addClass('ui-state-disabled');// item.setEnabled(false);
							else $item.removeClass('ui-state-disabled');// item.setEnabled(true);
                            break;
                        default:
							// 選択フォルダが一番親のワークorバーチャルフォルダが選択されていないときは使用できないようにする
                            if (ParantNdFlg) {
                                $item.addClass('ui-state-disabled');// item.setEnabled(false);
                                break;
                            }
                            switch (key) {
                                case "mnuRename":    //名前変更
								    if (EditLock || HideDel) {
									    $item.addClass('ui-state-disabled');// item.setEnabled(false);
								    } else {
                                        var v = checkEnableMenu(nd, "rename");
									    if (v) { $item.removeClass('ui-state-disabled');} else {$item.addClass('ui-state-disabled');}// item.setEnabled(v);
								    }
                                    break;
                                case "mnuDeleteFolder":    //フォルダ削除
								    if (EditLock || HideDel) {
									    $item.addClass('ui-state-disabled');// item.setEnabled(false);
								    } else {
									    var v = checkEnableMenu(nd, "delete");
									    if (v) { $item.removeClass('ui-state-disabled');} else {$item.addClass('ui-state-disabled');}// item.setEnabled(v);
								    }
								    if (bfolderdeleting) $item.addClass('ui-state-disabled');// item.setEnabled(false);
                                    break;
                                case "mnuPermission":    //ファイル登録時の権限設定
								    if (EditLock || HideDel) $item.addClass('ui-state-disabled');// item.setEnabled(false);
								    else $item.removeClass('ui-state-disabled');// item.setEnabled(true);
                                    break;
                                case "mnuCut":    //切り取り
								    if (EditLock || HideDel) {
									    $item.addClass('ui-state-disabled');// item.setEnabled(false);
								    } else {
									    var v = checkEnableMenu(nd, "delete"); //	切り取りは削除と同じ
									    if (v) { $item.removeClass('ui-state-disabled');} else {$item.addClass('ui-state-disabled');}// item.setEnabled(v);
								    }
								    if (bfolderdeleting) $item.addClass('ui-state-disabled');// item.setEnabled(false);
                                    break;
                                default:
                                    $item.removeClass('ui-state-disabled');// item.setEnabled(true);
                                    break;
                            }
                            break;
                    }
                });
            }
			// ポップアップイベントの表示を調整する
			function JudgeMnuItemDisable(nd, mnuParaID)
			{
				if (getTreeType() == 1) {
					//分類フォルダは常に表示
					return true;
				}
				var mnuID;
				var strIndex;
				var item;
				var ndTxt = "";
				var cmd;
				var i;
				var ParantNdFlg = false;
				
				var strKYOUYU_vf_Lock = window.document.getElementById("p_KYOUYU_vf_EditOff").defaultValue;
				var strPathNods = fullpath.split("\\");
				var EditLock = false;
				var HideDel = false;
				var isSharedFolder = false;

				var ts = igtree_getTreeById("wt");

				// メニューIDを求める
				strIndex = mnuParaID.lastIndexOf("M");
				if (strIndex != -1) {
					mnuID = mnuParaID.substring(0, strIndex);
				}
				else {
					mnuID = mnuParaID;
				}
				
				// 選択フォルダの文字列を調べる
				if ((nd != null) && (nd != undefined)) {
					ndTxt = nd.Element.innerText;
					
					// 親フォルダかどうか調べる
					var path = GetNodeFullPath(nd);
					if (Trim(ndTxt) == Trim(path))
						ParantNdFlg = true;
				}
				
				// 共有フォルダの編集が不可になっているかどうかチェック
				if (strPathNods[0] == "共有よく使う検索") {	//共有バーチャル
					isSharedFolder = true;
				    if (strKYOUYU_vf_Lock == "1")
				    	EditLock = true;
				    if (ro)
				        HideDel = true;
                }
				if (strPathNods[0] == "共有マーク") {	//共有マーク	
					isSharedFolder = true;
					if (ro)
				        HideDel = true;
				}

				// ポップアップアイテム20個に対して処理を行う
				for (i = 1; i <= 20; i ++) {
					item = igmenu_getItemById(mnuID + "_" + i);

					if ((item != null) && (item != undefined)) {

						cmd = item.Element.innerText;
						// 選択されたファイルがないとき
						if (cmd == "") {
							// do nothing
						}
						else if (cmd == "フォルダに対する権限設定...") {	//	フォルダに対する権限設定
							if (isSharedFolder) {
								if (EditLock || HideDel) item.setEnabled(false);
								else item.setEnabled(true);
							} else {
								item.setEnabled(false);
							}
						}
						else if (cmd == "貼り付け") {   //貼り付け
							// 貼り付け元が指定されていないときは使用できないようにする
							if (window.parent.document.getElementById("p_CopyFolderPath").value == "") {
								item.setEnabled(false);
							}
							else if (EditLock || HideDel) {
								item.setEnabled(false);
							}
							else {
								item.setEnabled(true);
							}
						}
						else if ((cmd == "上へ移動")) {      //上へ移動
						}
						else if ((cmd == "下へ移動")) {      //下へ移動
						}
						else if ((cmd == "新規作成（下層）")) {      //新規作成（下層）
						}
						else if ((cmd == "新規作成（同層）")) {      //新規作成（同層）
						}
						else if ((cmd != "新規作成")) {      //新規作成
							// 選択フォルダが一番親のワークorバーチャルフォルダが選択されていないときは使用できないようにする
							if (ParantNdFlg) item.setEnabled(false);
							//else if (EditLock == true)	item.setEnabled(false);

							else if ((cmd == "名前変更 F2")) {        //名前変更
								if (EditLock || HideDel) {
									item.setEnabled(false);
								} else {
									var v = checkEnableMenu(nd, "rename");
									item.setEnabled(v);
								}
							}
							else if ((cmd == "フォルダ削除")) {  //フォルダ削除
								if (EditLock || HideDel) {
									item.setEnabled(false);
								} else {
									var v = checkEnableMenu(nd, "delete");
									item.setEnabled(v);
								}
								if (bfolderdeleting) item.setEnabled(false);
							}
							else if ((cmd == "ファイル登録時の権限設定...")) {    //ファイル登録時の権限設定
								if (EditLock || HideDel) item.setEnabled(false);
								else item.setEnabled(true);
							}
							else if ((cmd == "切り取り")) {    //切り取り
								if (EditLock || HideDel) {
									item.setEnabled(false);
								} else {
									var v = checkEnableMenu(nd, "delete"); //	切り取りは削除と同じ
									item.setEnabled(v);
								}
								if (bfolderdeleting) item.setEnabled(false);
							}
							else item.setEnabled(true);
						}
						else {
							if (EditLock || HideDel) item.setEnabled(false);
							else item.setEnabled(true);
						}
						
					}
				}
			}
			//	共有よく使う検索フォルダ、もしくは共有マークフォルダならTrueを返す
			function checkSharedFolder(cpath) {
				if (cpath.indexOf("共有よく使う検索") == 0) {
					return true;
				}
				if (cpath.indexOf("共有マーク") == 0) {
					return true;
				}
				return false;
			}
			//	分類フォルダで管理用用メニューを利用かのうか
			function checkEnableMenu(nd, action) {
				var pnd = nd;
				var on = GetOriginalName(nd);
				if (document.getElementById('p_TreeType').value == "chFolderSf") {
					if (on == "(その他)" || on == "(選択しない)") {
						//	その他フォルダもしくは選択しないフォルダ
						return false;
					}
				} else {
					// 個人ワーク||個人バーチャル||共有ワーク||共有バーチャルの時はチェックしない
					return true;
				}
				//トップフォルダを取得
				while (nd.getParent() != null) {	
					nd = nd.getParent();
				}

				var v = nd.getTag();
				if (!v) return false;
				var v2 = v.split("|");
				if (v2.length < 3) return false;
				if (action == "createfolder") {
					if (v2[1] == "1") {
						return true;
					} else {
						return false;
					}
				} else if (action == "move" || action == "rename" || action == "delete") {
					if (pnd.getParent() == null) {
						return false;
					}
					if (v2[2] == "1") {
						return true;
					} else {
						return false;
					}
				}
				return true;
			}
			//	選択フォルダに対する Postback コマンド実行
			function SelectedFolderOp(op, arg)
			{
				var ts = igtree_getTreeById("wt");
				var nd = ts.getSelectedNode();
				var d = parent.frames["work"].document;
				if (d==null) {
					parent.ShowAlertDialog('D218', "コマンド実行で予期せぬエラー");  //コマンド実行で予期せぬエラー
					return;
				}
				var path = GetNodeFullPath(nd);
				d.getElementById("p_op").value = op;
				d.getElementById("p_result").value = arg;
				d.getElementById("p_args").value = path;
				d.getElementById("frm").submit();
            }
			var oktogo;
			var retv;
			function DownloadFolder()
			{
			    var DlFType = document.getElementById("DlFType");
				var args = new Array();
                //alert("DownloadFoler");
				args[0] = "0";						            // 取り出し後に開かない
				args[1] = cnfw_loadLastGetPath();	            // 取り出し先
				args[2] = cnfw_loadLastGetType(DlFType.value, 1);  // ファイル形式
				args[5] = window;
				var p=new Array();
                var l = cnfw_loadWinPosition("get", p);
				retv = false;
                window.focus();
                oktogo = false;
                //window.open("getfolderstat.aspx?t=1", "_blank", true);
                parent.cnfwShowModalDialog("get.aspx?mode=1", args, "scroll:no;status:no;dialogLeft:" + p[0] + ";dialogTop:" + p[1] + ";dialogWidth:660px;dialogHeight:250px", afterGetAspx);
                setTimeout(waitafter, 100);
            }
            var arg1;
            var arg2;
            function waitafter() {
				if (oktogo == true) {
					if (retv) {
						getfolderpath = AddYen(unescape(arg1));
						var mType = arg2;
						cnfw_saveLastGetPath(unescape(arg1));
						cnfw_saveLastGetType(mType, 1);
						var ts = igtree_getTreeById("wt");
						var nd = ts.getSelectedNode();
						var fname = nd.getText();
						//window.open("getfolderstat.aspx?t=1", "_blank", true);
						if (CheckPath(getfolderpath + fname))
							cnfw_DownloadFolder(window.parent, mType);
					}
                    return;
                }
                setTimeout(waitafter, 100);
            }
            function afterGetAspx(returnValue, args) {
                //alert("afterGetAspx");
                arg1 = args[1];
				arg2 = args[2];
                retv = returnValue;
				oktogo = true;
                returnValue = false; //for DEBUG
				if (returnValue == true) {// never true
					getfolderpath = AddYen(unescape(args[1]));
					var mType = args[2];
					cnfw_saveLastGetPath(unescape(args[1]));
					cnfw_saveLastGetType(mType, 1);
					var ts = igtree_getTreeById("wt");
					var nd = ts.getSelectedNode();
					var fname = nd.getText();
					if (CheckPath(getfolderpath + fname))
							cnfw_DownloadFolder(window.parent, mType);
				}
            }
			function CheckPath(path)
			{
				var w = parent.frames["work"];
				if (w==null) {
					parent.ShowAlertDialog('D219', "コマンド実行で予期せぬエラー");  //コマンド実行で予期せぬエラー
					return false;
				}
				if (!w.IsFolderExists(path)) {
					return true;
				}
				path = path.replace(/\\/g, "\\\\");
				if (ShowCnDialog('D125', { negative: 'true', title:  "上書き確認", type: 'confirm', message: "フォルダ $1 は存在します。上書きしますか？".replace("$1", path)}) == 1)   //"フォルダ "+path+" は存在します。上書きしますか？", 上書き確認
					return true;
				else
					return false;
			}
            //  件数表示のための再描画
		    function redrawFolder(nodeinfo) {
		        document.getElementById("p_redrawForCount").value = nodeinfo;
		        frmTree.submit();
		        return true;
		    }
            function RefreshFolder()
            {
                var cpath = parent.g_currentpath;
	            //  属性検索時は処理しない（currentpath==""のときは属性検索時）
	            if (cpath.length == 0) {
    	            return true;
	            }

	            //  よく使う検索・マークフォルダなら、選択フォルダを更新
	            if (
	                (cpath.indexOf("よく使う検索") == 0) 
	                || (cpath.indexOf("共有よく使う検索") == 0)   // 個人バーチャル||共有バーチャル
	                ||(cpath.indexOf("マーク") == 0) 
	                || (cpath.indexOf("共有マーク") == 0)) {   // 個人ワーク||個人バーチャル
	                UpdateFolder(cpath);
    	            return true;
	            }

                //  分類フォルダなら、最上位フォルダを選択してフォルダを更新
                //  現在選択しているノードをまず探す
	            var ts = igtree_getTreeById("wt");
	            var nd = ts.getSelectedNode();

	            //  現在のノードのTopを探し、そのIDを取得する
	            var pNd = nd.getParent();
	            var pId = nd.Id;
	            while(pNd != null) {
	                pId = pNd.Id;
		            pNd = pNd.getParent();
	            }
	            //  Topを選択させて、Ctrl+F5を動作させる
                igtree_setSelectedNode(ts.Id,pId);
                //  setSelectedNodeによりパスが代わっているので、改めてパス指定
                UpdateFolder(parent.g_currentpath);
	            return true;
            }
			function UpdateFolder(path)
			{
				document.getElementById("p_UpdateFolderPath").value = path;
				frmTree.submit();
			}
			function GetOriginalName(nd)
			{
			    return nd.getTag().replace(/\*ap\*/g, "'").split("|")[0];
			}
			function GetNodeFullPath(nd)
			{
				if (nd == null) {
					return "";
				}
				var path = GetOriginalName(nd);
				while (nd.getParent()!=null) {
					nd = nd.getParent();
					path = GetOriginalName(nd) + "\\" + path;
				}
				if ((path+"/").indexOf("\\(選択しない)/") > 0)    //(選択しない)
				    path = path.substring(0, path.lastIndexOf("\\(選択しない)"));   //(選択しない)
				return path;
			}
			//	メール通知一覧ウィンドウを開く
			function ShowMailSetting(strFullPath) 
			{
				if (!strFullPath) strFullPath = "";
				strFullPath = encodeURIComponent(strFullPath.replace(/&/g, "&amp;"));
			    var w = window.open("mailset.aspx?miid=0&pid=0&fullpath="+strFullPath, "_blank", "width=600,height=610,dependent=yes", true);
				if (w == undefined)
				    return;
				var d = new Date();
				window.parent.AddWindow(w, "mailsetwin" + d.toUTCString());
				return false;
			}
			function cancelEv(){
				window.event.returnValue = false;
                window.event.cancelBubble = true;
                window.event.preventDefault();
			}
            function keyEv() {
                if (document.readyState != 'complete' && document.readyState != 'interactive') return;
                
				switch (window.event.keyCode) {
				case 112: //    F1
                    if (window.event.ctrlKey) parent.cnfwonhelp();
					return;
				case 116: //	F5
					if (GetEnabled('F025BU1012')) {
						cnfw_UpdateMainList(window.parent);
					}
					cancelEv();
					window.event.keyCode = 0;
					return;
				case 27: // ESC
					if (parent.frames["main"].cancelQuery) {
						parent.frames["main"].cancelQuery();
					}
					cancelEv();
					window.event.keyCode = 0;
					return;
				default:
				}

			    var	fl = window.parent.main;
                if (fl.document.readyState != 'complete' && fl.document.readyState != 'interactive') return;
			    var path;
				if (window.event.ctrlKey) {
				    switch (window.event.keyCode) {
                    case 67:    //  Ctrl+C
                    	if (!GetEnabled('F006ME' + getTreeType() + '005')) {
                    		cancelEv();
                    		window.event.keyCode = 0;
                    		return;
                    	}
                        if (event.srcElement.id=="wt_tb") {
                            //  NodeEdit
                            return;
                        }
                    	path = GetSendPath();
                        if (path.indexOf("\\") > 0) {
					        window.parent.document.getElementById("p_CopyFolderPath").value = path;
					        window.parent.document.getElementById("p_CopySubFolder").value = false;
					        window.parent.document.getElementById("p_Cut").value = "";
					    }
					    cancelEv();
					    break;

                    case 71:    //  Ctrl+G
                    	if (!GetEnabled('F006ME' + getTreeType() + '006')) {
                    		cancelEv();
                    		window.event.keyCode = 0;
                    		return;
                    	}
                    	path = GetSendPath();
                        if (path.indexOf("\\") > 0) {
					        window.parent.document.getElementById("p_CopyFolderPath").value = path;
					        window.parent.document.getElementById("p_CopySubFolder").value = true;
					        window.parent.document.getElementById("p_Cut").value = "";
					    }
					    cancelEv();
					    break;

				    case 76:	//	Ctrl+L
					    fl.SaveSelectedFiles();
					    fl.SetViewMode(parent.g_dmode);
					    fl.CommandPostback("view");
					    cancelEv();
					    break;

				    case 77:	//	Ctrl+M
					    fl.SaveSelectedFiles();
					    fl.SetViewMode(1);
					    fl.CommandPostback("view");
					    cancelEv();
					    break;

				    case 78:	//	Ctrl+N
					    fl.SaveSelectedFiles();
					    fl.SetViewMode(4);
					    fl.CommandPostback("view");
					    cancelEv();
					    break;

				    case 82:	//	Ctrl+R
				    	if (GetEnabled('F025BU1012')) {
				    		cnfw_UpdateMainList(window.parent);
				    	}
				    	cancelEv();
					    break;

				    default:
                        var w = window.parent;
                        w.keyEvProc(window.event);
                        break;
				    }
	            }  else if (window.event.altKey) {
				    switch (window.event.keyCode) {
                    case 18:	//	Alt
                        return;
            		
		            case 48:    //  Alt+0
		            case 49:    //  Alt+1
		            case 50:    //  Alt+2
		            case 51:    //  Alt+3
		            case 52:    //  Alt+4
		            case 53:    //  Alt+5
		            case 54:    //  Alt+6
		            case 55:    //  Alt+7
		            case 56:    //  Alt+8
		            case 57:    //  Alt+9
		            case 66:    //  Alt+B
		            case 74:    //  Alt+J
		            case 88:    //  Alt+X
                    case 89:    //  Alt+Y
                        var w = window.parent;
                        w.keyEvProc(window.event);
                        break;

				    case 78:	//	Alt+N
					    var ts = igtree_getTreeById("wt");
					    var nd = ts.getSelectedNode();
					    if (nd == null) break;
					    ShowMenu(nd);
					    cancelEv();
					    window.event.keyCode = 0;
					    break;

				    default:
				    }
				}
			}
			function cancelEv(){
				window.event.returnValue = false;
                window.event.cancelBubble = true;
                window.event.preventDefault();
			}
			function init() {
			    if (cnfw_CheckDirectAccess() == false) return;
			    var w = parent.frames["main"];
			    if (w.document.location.href.search(/blank.htm$/) != -1 || w.document.location.href.search(/empty.aspx$/) != -1)
			        w.document.location.replace("filelist.aspx");

				var fullpath = UpdateCurrentPath();
				if (!cnfw_CheckDirectAccess())
					return;
				var tree = igtree_getTreeById("wt");
				tree.AllowDrag = false;
				var p_RO = document.getElementById("p_RO");
				if (p_RO.value == "true") ro = true;
				GetTopicPathInfo(fullpath);
			}
			//	デフォルトのポップアップメニューを非表示にする
			document.oncontextmenu = function()
			{
			    var $node = $(event.srcElement).closest('div[igTag]');
			    if ($node.length == 0) return false;
			    var id = $node.attr('id');
			    var ts = igtree_getTreeById("wt");
			    ts.setSelectedNode(id);
			    m_isRightClick = true;
			    igtree_setSelectedNode("wt", id);
			    var nd = ts.getSelectedNode();
			    ShowMenu(nd);
			    return false;
			}

			var _tmpGtp;
			function regetTopicPathInfo() {
			    var fullpath = UpdateCurrentPath();
			    GetTopicPathInfo(fullpath);
			}
            function GetTopicPathInfo(path)
            {
                clearTimeout(_tmpGtp);
                var w = frames["topicpath"];
                if (!w || !w.document.getElementById("activepath") || document.readyState != 'complete') {
                    _tmpGtp = setTimeout(regetTopicPathInfo, 1000);
                    return;
                }
	            w.document.getElementById("activepath").value = encodeURI(path);
	            w.document.getElementById("topicpath").value = "";
	            w.document.getElementById("form1").submit();
            }

            function RecieveTopicPathInfo(arg, context)
            {
                var topicpath2 = document.getElementById("topicpath2");
                topicpath2.value = arg.replace("<strong>", "[").replace("</strong>", "]");
                topicpath2.title = topicpath2.value;
            }
            function onDragging()
            {
                var w = window.parent;
                w.onDragging(event.clientX, "leftPane");
            }
            function ShowHideConditionPane(visible) {
                if (parent == self) return;
                try {
                    parent.SetConditionVisible(visible);
                } catch (e) { }
            }
            function wt_AfterBeginNodeEdit(tn, nodeid) {
            	var elem = document.activeElement;
            	elem.style.width = 100;
            }
            function AddYen(path) {
                if (path.slice(-1) == '\\'){
                    return path;
                } else {
                    return path + '\\';
                }
            }
            function GetTargetPath() {
                return getfolderpath;
            }
