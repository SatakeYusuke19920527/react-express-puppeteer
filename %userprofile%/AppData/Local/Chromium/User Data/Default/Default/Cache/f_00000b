<SCRIPT language='javascript'>ShowHideConditionPane(false);</SCRIPT><script language='javascript'>try{__igmenu_mouseover = igmenu_mouseover;igmenu_mouseover = function (table, evnt, viaMouse) {__igmenu_mouseover(table, evnt, viaMouse); var item; try{item = igmenu_getTblRow(evnt)} catch(e){return;};if (!item) return; var tag = item.getAttribute('igTag');if (!tag) return; var s = tag.split(',');if (s.length > 1) {tag = s[1];} if (IsHelpMode()) ShowHelp(tag);}} catch(e) {}try{__igtree_mouseover = igtree_mouseover; igtree_mouseover = function(evnt,tn) { __igtree_mouseover(event,tn);if (IsHelpMode()) ShowHelp(tn);}	}catch(e) {}try{__igtbar_mouseOver = igtbar_mouseOver; igtbar_mouseOver = function(src,evnt, enabled) { __igtbar_mouseOver(src, evnt, enabled);var b=igtbar_getItemById(src.id);if (b && b.Tag) {var tag = b.Tag ;if (IsHelpMode()) ShowHelp(tag);}} }catch(e) {}</script><SCRIPT language='javascript'>m_isGUIEditMode=false;</SCRIPT></form>
        <script type="text/javascript">
        <!--
            $(function () {
                $('#frmTree').mouseup(function () {
                    var w = $(this).width();
                    if ($('body').width() < w)
                        parent.ResizeLeftPane(w + 20);
                });
                $(window).on("beforeunload",function(e){
                    if (_ajaxBusy) {
                        return "フォルダの件数取得中です"; //フォルダの件数取得中です
                    }
                    //$('*').css('cursor', 'wait');
                });
                $('*').css('cursor', '');
            });
            $(window).on('load', function () {
                igtree_loadcomplete();                //$('#treeframe').height($('#BBB').height() - 20);

                if (m_startNodeEdit) {
                    StartNodeEdit();
                } else {
                    $('#T_wt').focus();
                }
                if ($("#p_TreeType").val() != 'chFolderVf' && $("#p_TreeType").val() != 'chFolderWf') return;
                //  よく使う検索、もしくはマークフォルダであればフォルダ件数表示
                getFolderCount();
            });
            //  検索結果(FileList.aspx)からの通知でフォルダ件数を更新する
            function updateFolderCount(fdrpath, cnt) {
                var ndid = "";
                var found = false;
                var targets = $("#p_countFolderNames").val().split("|");
                if (parent.g_currentpath != fdrpath) return;

                $.each(targets, function (i, val) {
                    if (val == "") return true;
                    ndid = "wt" + val;
                    var nd = igtree_getNodeById(ndid);
                    var str = GetNodeFullPath(nd)
                    if (str == fdrpath) {
                        found = true;
                        return false;
                    }
                });

                if (!found) return;

				$("#" + ndid).find('.badge').each(function () {
					$(this).remove();
				});
                $("#" + ndid).children().last().append('<span class="badge">(' + cnt + ')</span>');
                setBadgeClickProc();
            }
            //  フォルダ件数表示
            function getFolderCount() {
                var filter = m_expandNodeId;
                m_expandNodeId = null;
                var fdrs = new Array();
                var targets = $("#p_countFolderNames").val().split("|");
                $.each(targets, function (i, val) {
                    if (val == "") return true;
                    var ndid = "wt" + val;
                    var nd = igtree_getNodeById(ndid);
                    if (!isExpanded(nd)) return true;
                    if (filter && !ndid.startsWith(filter)) return true;    //メモリ中のノードを展開だけしたときに下層だけUpdateするように
                    if ($("#" + ndid).children().last().get(0).innerHTML.indexOf('class="badge"') > 0) {//    表示済の件数は一旦非表示にしてしまう
                        $("#" + ndid).children().last().children().first().remove();
                    }
                    var txt = nd.getText();
                    txt = $("<span>" + txt + "</span>").contents(":first").text();//件数の部分を切り取る
                    var str = GetNodeFullPath(nd)
                    fdrs.push(ndid + "|" + str);
                });
                if (fdrs.length == 0) {
                    return;
                }

                var cntfdr = fdrs.join(",");
                var treeType = $("#p_TreeType").val();
                _ajaxBusy = true;
                $.ajax({
                    url: "fwajaxproc.aspx",
                    type: 'POST',
                    async: true,
                    data: {
                        'cmd': 'countfolder',
                        'fdrs': cntfdr,
                        'foldertype': treeType
                    },
                    dataType: 'json',
                    success: cntFolderCompleted,
                    error: function (httpReq, textStatus, errorThrown) {
                        window.status = textStatus;
                    },
                    complete: function () {
                        _ajaxBusy = false;
                    }
                });
            }
            //  フォルダ件数カウント完了
            function cntFolderCompleted(e) {
                var ret = e.data;
                var retdata = ret.split(",");
                for (var i = 0; i < retdata.length; i++) {
                    var s = retdata[i].split("|");
                    var id = s[0];
                    if (id.length == 0) continue;
                    var num = s[1];

					$("#" + id).find('.badge').each(function () {
						$(this).remove();
					});
                    $("#" + id).children().last().append('<span class="badge">(' + num + ')</span>');
                }
                setBadgeClickProc();
            }
            //  数値の箇所をクリックでもノード選択となるように
            function setBadgeClickProc() {
                $('.badge').on('click', function () {
                    var ts = igtree_getTreeById("wt");
                    var ndid = $(this).closest('div').attr('id');
                    var nd = igtree_getNodeById(ndid);
                    ts.setSelectedNode(nd);
                });
            }
            //  ツリー中のノードが展開されているか（画面上見えているか）を返す
            function isExpanded(nd) {
                if (!nd) return false;
                var ret = true;
                for (var i = 0; i < 100; i++) {//無限ループにならないように100階層までチェック
                    nd = nd.getParent();
                    if (nd == null) {
                        break;
                    }
                    if (!nd.getExpanded()) {
                        //  閉じている
                        ret = false;
                        break;
                    }
                }
                return ret;
            }

            //-->
        </script>
