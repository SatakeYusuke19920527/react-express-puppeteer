			function mDoSearch() {
			    if (m_tmr) clearTimeout(m_tmr);
			    m_tmr = null;
			    var bReady = false;
			    try {
                    var docmain = parent.frames["main"].document;
                    if (( docmain.readyState == "complete" || docmain.readyState == "interactive" )
                        && docmain.location.href.toLowerCase().indexOf("filelist.aspx") > 0) 
                        bReady = true;
			    } catch (e) {}
			    if (!bReady) {
			        m_tmr = setTimeout(mDoSearch, 500);
			        return true;
			    }
	            if (parent.g_searching) {
			        return true;
			    }
			    var ts = igtree_getTreeById("wt");
			    var nd = ts.getSelectedNode();
			    if (!nd) return;
                if (nd.Id != m_tmrNodeId) {
                    nd = igtree_getNodeById(m_tmrNodeId);
                    ts.setSelectedNode(nd);
                }
			    var pp = fullpath;
			    fullpath = UpdateCurrentPath();
			    if (fullpath == "") {
			        var nd = igtree_getNodeById(m_tmrNodeId);
			        if (nd)
			            nd.setExpanded(true);
			        return true;
			    }
			    if (fullpath.indexOf("\\") < 0) {
                    //  フォルダのトップをクリックしたので検索せずに結果のクリアだけ行う
			        GetTopicPathInfo(fullpath);
			        parent.frames["main"].document.location.href = "FileList.aspx?empty=true";
			        return true;
			    }
			    if (pp.length > fullpath.length && pp.indexOf(fullpath) > -1) {
			        var nodes = ts.getNodes();
			        for (var n = 0; n < nodes.length; ++n) {
			            var ndTop = nodes[n];
			            if (fullpath.indexOf(ndTop.getText() + "\\") > -1) {
			                var nd = igtree_getNodeById(m_tmrNodeId);
			                nd.setExpanded(false);
			                ts.setSelectedNode(ndTop);
			                return;
			            }
			        }
			    }
			    GetTopicPathInfo(fullpath);
			    parent.g_keepconditionpane = false;
			    if (m_tmrRightClick) {
			        //	右クリックメニューからの実行（かつ検索条件設定画面から「条件設定」ボタンで来たものではない）の場合は内部的に検索しないように
			        parent.frames["main"].document.location.replace("searching.aspx?empty=true");
			    } else {
			        parent.frames["main"].document.location.replace("searching.aspx");
			        window.status = "検索実行中..."; //検索実行中...
			        if (parent.frames["condition"].HiddenControls != undefined) parent.frames["condition"].HiddenControls();
			        //  選択場所が変わったので、(無効なメニューの実行を防ぐため)メニューは消しておく
			        //cnfw_igmenu_hideMenu("mnuPopup");
			    }
			    parent.g_searching = true;

			}
			function DoSearch(nodeId, rightClick) {
			    if (m_tmr && m_tmrNodeId==nodeId) return;
			    if (m_tmr) clearTimeout(m_tmr);
			    if (nodeId == null) {                       //  from UpdateMainList() in vfcondition.aspx
			        var ts = igtree_getTreeById("wt");
			        var nd = ts.getSelectedNode();
			        nodeId = nd.Id;
			    }
			    m_tmrNodeId = nodeId;
			    m_tmrRightClick = rightClick;
			    mDoSearch();
				return true;
			}
			function wt_AfterNodeSelectionChange(treeId, nodeId, button) {
				try {
					if (m_isRightClick) {
						//	右クリックで来た場合
						return DoSearch(nodeId, true);
					}
					if (!m_isClicked) {
						//	キーボード操作で来た場合
						return DoSearch(nodeId, false);
					}
				} finally {
					m_isRightClick = false;
					m_isClicked = false;
				}
				return true;
			}
			function UpdateCurrentPath()
			{
				var ts = igtree_getTreeById("wt");
				var nd = ts.getSelectedNode();
				if (nd == null)
					return "";

				fullpath = GetNodeFullPath(nd);
				if (fullpath=="") {
				} else {
					parent.g_activepath = fullpath;
					parent.g_currentpath = fullpath;
				}
				return fullpath;
			}
			function getTreeType() {
				var treeType = "1";
				switch (document.getElementById("p_TreeType").value) {
					case "chFolderVf":
						treeType = "2";
						break;
					case "chFolderWf":
						treeType = "3";
						break;
				}
				return treeType;
			}
			function wt_KeyDown(oTree, keyCode) {
                if (document.readyState != 'complete' && document.readyState != 'interactive') return;
                
				switch (keyCode) {
				case 113:   //F2
					if (!GetEnabled('F006ME' + getTreeType() + '001')) {
						return;
					}
					if (getTreeType() == 1 && document.getElementById('p_isAdmin').value != "true") {
						//	管理者でないならF2を無効に（120081）
						return;
					}

					if (IsEnableEditSelectedNode()) {
						StartNodeEdit();
						return true;
					}
					break;
			    default:
			    }
			}
			function wt_DragLeave(oTree, oNode, oDataTransfer, oEvent){
			}
			function wt_Drop(oTree, oNode, oDataTransfer, oEvent){
				fullpath = GetNodeFullPath(oNode);
				parent.g_activepath = fullpath;
				igtree_cancelPostBack("wt");
				return true;
			}
			function wt_BeforeNodeSelectionChange(treeId, oldNodeId, newNodeId){
				if (parent.g_searching) {
					return true;
				}
			}
			function wt_NodeClicked(treeId, nodeId, button) {
				var nd = igtree_getNodeById(nodeId);
				if (nd == null)
					return true;
				
				m_isClicked = true;
				fullpath = GetNodeFullPath(nd);
				if (button == 2) {
					// 右クリックならばメニューをポップアップ
					if (document.getElementById("mnuPopup")) {
						ShowMenu(nd);
					}
					//	右クリックの時はSelectionChangeの時だけ検索が走るように後のイベントで処理
					m_isRightClick = true;
					nd.setSelected(true);
					return true;
				} else {
					m_isRightClick = false;
					nd.setSelected(true);
				}

				DoSearch(nodeId, false);
				return true;
			}
var _tmr;
var _$mnu;
			function ShowMenu(nd) 
			{
				//JudgeMnuItemDisable(nd, 'mnuPopup');					// Item状態の確認処理
                setMenuEnabled(nd);
                var $menu = $('#mnuPopup');
                var x = event.clientX;
                var y = event.clientY + 10;
                $menu.show();
                var $ul = $('#contextmenu');
                $ul.menu({
                    create: function () {
                        $widget = $(this).menu('widget');
                        $widget.css({
                            'outline': 'none'
                        });
                    },
                    focus: function (event, ui) {
                        clearTimeout(_tmr);
                        var mh2 = $widget.height();
                        var mw2 = $widget.width();
                        var bodyHeight = $('body').height();
                        var bodyWidth = $('body').width();
                        if (y + mh2 > bodyHeight) {
                            y = bodyHeight - mh2;
                            if (y < 0) y = 0;
                        }
                        if (x + mw2 > bodyWidth) {
                            x = bodyWidth - mw2;
                            if (x < 0) x = 0;
                        }
                        $menu.offset({ left: x, top: y }).show();
                        _$mnu = ui.item;
                    },
                    blur: function (event, ui) {
                        _tmr = setTimeout(cnfw_HideContextMenu, 200);
                        _$mnu = ui.item;
                    },
                    select: function (event, ui) {
                        _$mnu = ui.item;
                        event.preventDefault();
                        var url = $('a', _$mnu).attr('href');
                        if (url == undefined) url = "";
                        if (url.length == 0) {
                            var $a = $('a', _$mnu);
                            url = "javascript:cnfw_MenuHandler2('" + $a.attr('data-key') + "','" + $a.attr('data-resid') + "')";
                        }
                        if (url == location.href + "#") {
                            url = "";
                        }
                        if (_$mnu.hasClass('ui-state-disabled')) {
                            url = "";
                        }
                        if (url.length > 0) {
                            cnfw_HideContextMenu();
                            try {
                                if(url == null || url.length == 0)
                                    return;
                                var newUrl=url.toLowerCase();
                                if(newUrl.indexOf("javascript") != -1)
                                    eval(url);
                                else 
                                    windo.open(url, "_blank");
                            } 
                            catch (x) {
                            }
                        }
                        event.returnValue = false;
                        event.cancelBubble = true;
                        return false;
                    }
                }).focus();
                $('li', $ul).on({
                    'contextmenu': function () {
                        return false;
                    },
                    'mouseover': function () {
                        $(this).addClass('hover');
                    },
                    'mouseleave': function () {
                        $(this).removeClass('hover');
                    }
                });
			}
            function cnfw_HideContextMenu() {
                var $ul = $('#mnuPopup');
                $ul.hide();
            }
			function lbrFolder_AfterGroupSelected(oListbar, oGroup, oEvent){
